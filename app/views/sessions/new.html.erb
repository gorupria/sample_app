<h1>Sign in</h1>
<!--at 0902, timestamp: 11:00 -->
<%= form_for(:session, :url => sessions_path) do |f| %>
  <div class="field">
	<%= f.label :email %><br/>
	<%= f.text_field :email, class:'roundField' %>
  </div>
  <div class="field">
	<%= f.label :password %><br/>
	<%= f.password_field :password, class:'roundField' %>
  </div>
  <div class="actions">
  	<%= f.submit "Sign in" %>
  </div>
<% end%>

<p>New user? <%= link_to "Sign up now!", signup_path %></p>
